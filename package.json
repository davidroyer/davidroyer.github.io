{
  "name": "davidroyer",
  "version": "1.0.0",
  "description": "Nuxt.js Blog",
  "author": "David Royer <droyer01@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env HOST=0.0.0.0 PORT=8088 nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "HOST=davidroyer.github.io PORT=80 NODE_ENV=production nuxtent generate",
